<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/my_secondary"
    tools:context=".JobsActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/topToolbar482"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/my_primary"
        android:title="Jobs"
        android:titleTextColor="#FFFFFF" />

    <WebView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
        android:layout_below="@id/topToolbar482"
        android:layout_above="@+id/bottomToolbar482"
        android:id="@+id/jobsWeb482"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_alignParentBottom="true"
        android:id="@+id/bottomToolbar482"
        android:background="@color/my_primary"
        android:orientation="horizontal"
        android:gravity="center"
        app:menu="@menu/toolbarmenu">



    </LinearLayout>





</RelativeLayout>